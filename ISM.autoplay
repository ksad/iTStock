<Event>
<Name>On Click</Name>
<Args/>
<Script>local buttonAction = xButton.GetText(this);

if buttonAction == "Git pull" then
	Shell.Execute("AutoPlay\\htdocs\\batchs\\run_git_pull.bat", "open", "", "AutoPlay\\htdocs\\batchs", SW_HIDE, true);

	local pullError = TextFile.ReadToString("AutoPlay\\htdocs\\batchs\\Temp\\err.tmp");

	if String.Find(pullError, "error", 1, false) ~= -1 then
		Dialog.Message("Git pull error", pullError, MB_OK, MB_ICONSTOP, MB_DEFBUTTON1);
		Application.ExitScript();
	else
		local pullResponse = TextFile.ReadToString("AutoPlay\\htdocs\\batchs\\Temp\\pull.tmp");
		if String.Find(pullResponse, "CONFLICT", 1, false) ~= -1 then
			Dialog.Message("Git pull error", "A merge conflict was detected. Please fix it or clone the project.", MB_OKCANCEL, MB_ICONSTOP, MB_DEFBUTTON1);
			xButton.SetText("BTN_GIT_ACTION", "Git clone");
		else
			Dialog.Message("Git pull", "Pull done.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
			DialogEx.Close(this);
		end
	end
end

if buttonAction == "Git clone" then
	File.Copy("AutoPlay\\htdocs\\batchs\\run_git_clone.bat", _TempFolder, true, true, false, true, nil);
	Shell.Execute(_TempFolder.."\\run_git_clone.bat", "open", "", _SourceFolder, SW_SHOWNORMAL, true);
end</Script>
