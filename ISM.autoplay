<Event>
<Name>On Close</Name>
<Args/>
<Script>objectList = DialogEx.EnumerateObjects();
for i, object in pairs(objectList) do
	objectType = DialogEx.GetObjectType(object);
	if objectType == 3 and String.Find(object, "UCF", 1, false) ~= -1 then -- UCF = User Create Form
		Image.Load(object, "AutoPlay\\htdocs\\images\\inputs\\input.png");
	end
end

Label.SetText("LB_CREATE_TITLE", Trans("user.dialog.title", "users"));
Label.SetText("LB_ACCESS_ROLES", Trans("user.permissions", "users"));
Label.SetText("LB_CHECKBOX", Trans("user.checkbox.active#", "users"));
Label.SetText("LB_SIGN", Trans("user.switch.signature#", "users"));

Input.SetText("IN_LASTNAME_UCF", Trans("user.input.lastname", "users"));
Input.SetText("IN_FIRSTNAME_UCF", Trans("user.input.firstname", "users"));
Input.SetText("IN_USERNAME_UCF", Trans("user.input.username", "users"));
Input.SetText("IN_PASSWORD_UCF", Trans("user.input.password", "users"));
Input.SetText("IN_EMAIL_UCF", Trans("user.input.email", "users"));

ComboBox.SetItemText("CB_ROLE", 1, Trans("user.combobox.role", "users"));
ComboBox.SetSelected("CB_ROLE", 1);

Image.SetVisible("IMG_NOTIFY_ROLE", false);

Image.Load("IMG_TOGGLE_YES_NO", "AutoPlay\\htdocs\\images\\icons\\switch_yes_no_idle.png");

tblLabelProps = {};
tblLabelProps.ColorNormal = Math.HexColorToNumber("FFFFFF");
tblLabelProps.ColorDown = Math.HexColorToNumber("FFFFFF");
tblLabelProps.ColorHighlight = Math.HexColorToNumber("FFFFFF");
tblLabelProps.ColorDisabled = Math.HexColorToNumber("FFFFFF");
Label.SetProperties("LB_SIGN", tblLabelProps);
Image.SetVisible("IMG_NOTIFY_SIGN", false);

Button.SetText("BTN_CREATE_USER", Trans("user.button.create", "users"));
Button.SetText("BTN_CANCEL", Trans("user.button.cancel", "users"));</Script>
