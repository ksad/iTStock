<Event>
<Name>On Click</Name>
<Args/>
<Script>local indexSelecteduser = ListBox.GetSelected("LBX_UPDATED_USERS")[1];
local dataSelecteduser = ListBox.GetItemData("LBX_UPDATED_USERS", indexSelecteduser);

local lastName = Input.GetText("IN_LASTNAME_UCF");
local firstName = Input.GetText("IN_FIRSTNAME_UCF");
local username = Input.GetText("IN_USERNAME_UCF");
local email = Input.GetText("IN_EMAIL_UCF");
local role = ComboBox.GetItemText("CB_ROLE", ComboBox.GetSelected("CB_ROLE"));

local LoadedImage = Image.GetFilename("IMG_TOGGLE_YES_NO");
local ImageStatus = String.SplitPath(LoadedImage);
local checkListSign = ImageStatus.Filename;
if checkListSign == "yes" then
	signature = "Yes"
else
	signature = "No"
end

local LoadedImage = Image.GetFilename("IMG_CHECKBOX");
local ImageStatus = String.SplitPath(LoadedImage);
local status = ImageStatus.Filename;
if status == "checkbox_on" then
	accountStatus = 1;
else
	accountStatus = 0;
end

if lastName == "" or lastName == Trans("user.input.lastname", "users") then
	Image.Load("IMG_LASTNAME_UCF", "AutoPlay\\htdocs\\images\\inputs\\input_mandatory.png");
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "Error616");
else
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "");
end

if firstName == "" or firstName == Trans("user.input.firstname", "users") then
	Image.Load("IMG_FIRSTNAME_UCF", "AutoPlay\\htdocs\\images\\inputs\\input_mandatory.png");
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "Error616");
else
	if Application.LoadValue("ItStock", "FORM_LAST_ERROR") == "" then
		Application.SaveValue("ItStock", "FORM_LAST_ERROR", "");
	end
end

if username == "" or username == Trans("user.input.username", "users") then
	Image.Load("IMG_USERNAME_UCF", "AutoPlay\\htdocs\\images\\inputs\\input_mandatory.png");
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "Error616");
else
	if Application.LoadValue("ItStock", "FORM_LAST_ERROR") == "" then
		Application.SaveValue("ItStock", "FORM_LAST_ERROR", "");
	end
end

if email == "" or email == Trans("user.input.email", "users") then
	Image.Load("IMG_EMAIL_UCF", "AutoPlay\\htdocs\\images\\inputs\\input_mandatory.png");
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "Error616");
else
	if Application.LoadValue("ItStock", "FORM_LAST_ERROR") == "" then
		Application.SaveValue("ItStock", "FORM_LAST_ERROR", "");
	end
end

if role == Trans("user.combobox.role", "users") then
	Image.SetVisible("IMG_NOTIFY_ROLE", true);
	Image.SetVisible("IMG_ADD_ROLE", false);
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "Error616");
else
	if Application.LoadValue("ItStock", "FORM_LAST_ERROR") == "" then
		Application.SaveValue("ItStock", "FORM_LAST_ERROR", "");
	end
end

if checkListSign == "switch_yes_no_idle" then
	tblLabelProps = {};
	tblLabelProps.ColorNormal = Math.HexColorToNumber("FF0000");
	tblLabelProps.ColorDown = Math.HexColorToNumber("FF0000");
	tblLabelProps.ColorHighlight = Math.HexColorToNumber("FF0000");
	tblLabelProps.ColorDisabled = Math.HexColorToNumber("FF0000");
	Label.SetProperties("LB_SIGN", tblLabelProps);
	Image.SetVisible("IMG_NOTIFY_SIGN", true);
	Application.SaveValue("ItStock", "FORM_LAST_ERROR", "Error616");
else
	if Application.LoadValue("ItStock", "FORM_LAST_ERROR") == "" then
		Application.SaveValue("ItStock", "FORM_LAST_ERROR", "");
	end
end

error = Application.LoadValue("ItStock", "FORM_LAST_ERROR");
if (error == "Error616") then
	Application.ExitScript();
end
local mySQLConnection = dbConnect();
if mySQLConnection ~= nil then
	SQL = "UPDATE `it_stock_manager`.`it_users` SET `Lastname`='"..lastName.."', `Firstname`='"..firstName.."', `Username`='"..username.."', `Email`='"..email.."', `Role`='"..role.."', `Sign_Check_List`='"..signature.."', `Active`='"..accountStatus.."', `Modified`=now(), `Modified_By`='"..CURRENT_USER.."' WHERE ID = '"..dataSelecteduser.."';";

	mySQLCursor, err = mySQLConnection:execute(SQL);
	if err then
		showMsgBox ("Error", Trans("user.msgbox.title.update", "users"), err, "OK");
		Application.ExitScript();
	else
		local indexSelecteduser = ListBox.GetSelected("LBX_UPDATED_USERS")[1];

		ListBox.DeleteItem("LBX_UPDATED_USERS", indexSelecteduser);
		ListBox.SelectItem("LBX_UPDATED_USERS", 1);

		showMsgBox ("Success", "", Trans("user.update.success", "users", {username}), "OK");
		local nItems = ListBox.GetCount("LBX_UPDATED_USERS");
		if nItems == 0 then
			DialogEx.Close(this);
		end
	end
end</Script>
