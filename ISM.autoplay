<Script>local frLang = RadioButton.GetChecked("RD_FR");
local enLang = RadioButton.GetChecked("RD_UK");

if enLang == false and frLang == false then
	Dialog.Message("Error", "Please select language.", MB_OK, MB_ICONINFORMATION, MB_DEFBUTTON1);
	Application.ExitScript();
elseif frLang then 
	lang = RadioButton.GetProperties("RD_FR").Value;
else
	lang = RadioButton.GetProperties("RD_UK").Value;
end

TextFile.WriteFromString("AutoPlay\\htdocs\\lang-properties\\default_lang.lang", lang, false);</Script>
