<Script>if e_Key == 9 then
	DialogEx.SetFocus("IN_EMAIL_UCF");
end

if e_Key == 13 then
	DialogEx.ClickObject("BTN_CREATE_USER");
end

local mySQLConnection = dbConnect();
if mySQLConnection ~= nil then
	local searchByUsername = Input.GetText("IN_USERNAME_UCF");

	if searchByUsername ~= "" and searchByUsername ~= Trans("user.input.username","users") then
		SQL = 'SELECT * FROM IT_USERS WHERE Username = "'..searchByUsername..'";';
		mySQLCursor = mySQLConnection:execute(SQL);

		row = mySQLCursor:fetch({},"a")

		tblImageProps = {};
		tblImageProps.Visible = true;
		if row then
			tblImageProps.ImageFile = "AutoPlay\\htdocs\\images\\icons\\error.png";
			tblImageProps.TooltipText = "User already exist";
		else
			tblImageProps.ImageFile = "AutoPlay\\htdocs\\images\\icons\\ok.png";
			tblImageProps.TooltipText = "Username available";
		end
		Image.SetProperties("IMG_NOTIFY", tblImageProps);
		mySQLCursor:close();
		mySQLConnection:close();
	end
end</Script>
