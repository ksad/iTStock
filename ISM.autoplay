<Args/>
<Script>Image.Load("IMG_LASTNAME_UCF", "AutoPlay\\htdocs\\images\\inputs\\input.png");
Image.Load("IMG_FIRSTNAME_UCF", "AutoPlay\\htdocs\\images\\inputs\\input.png");
Image.Load("IMG_USERNAME_UCF", "AutoPlay\\htdocs\\images\\inputs\\input.png");
Image.Load("IMG_EMAIL_UCF", "AutoPlay\\htdocs\\images\\inputs\\input.png");

local indexSelecteduser = ListBox.GetSelected("LBX_UPDATED_USERS")[1];
local dataSelecteduser = ListBox.GetItemData("LBX_UPDATED_USERS", indexSelecteduser);

local mySQLConnection = dbConnect();
if mySQLConnection ~= nil then
	SQL = "SELECT * FROM IT_USERS WHERE ID = '"..dataSelecteduser.."';";
	mySQLCursor = mySQLConnection:execute(SQL);
	row = mySQLCursor:fetch({},"a")

	if row then
		while row do
			lastname = row.Lastname;
			firstname = row.Firstname;
			username = row.Username;
			email = row.Email;
			modified = row.Modified;
			modifiedBy = row.Modified_By;
		
			Input.SetText("IN_LASTNAME_UCF", lastname);
			Input.SetText("IN_FIRSTNAME_UCF", firstname);
			Input.SetText("IN_USERNAME_UCF", username);
			Input.SetText("IN_EMAIL_UCF", email);
			if modified and modifiedBy then
				lastModification = modified;
				lastModificationBy = "("..modifiedBy..")";
			else
				lastModification = "--"
				lastModificationBy = "";
			end
			
			role = row.Role;
			ComboBox.SetSelected("CB_ROLE", ComboBox.FindItem("CB_ROLE", -1, LB_BYTEXT, role));

			if row.Sign_Check_List == "Yes" then
				signCheckList = Trans("user.switch.signature.yes", "users");
				Image.Load("IMG_TOGGLE_YES_NO", "AutoPlay\\htdocs\\images\\icons\\yes.png");
			else
				signCheckList = Trans("user.switch.signature.no", "users");
				Image.Load("IMG_TOGGLE_YES_NO", "AutoPlay\\htdocs\\images\\icons\\no.png");
			end

			if row.Active == "1" then
				status = Trans("user.checkbox.active", "users");
				Image.Load("IMG_CHECKBOX", "AutoPlay\\htdocs\\images\\icons\\checkbox_on.png");
			else
				status = Trans("user.checkbox.disabled", "users");
				Image.Load("IMG_CHECKBOX", "AutoPlay\\htdocs\\images\\icons\\checkbox_off.png");
			end

			Paragraph.SetText("PH_OLD_CONFIG", "***** "..Trans("user.update.old.infos", "users").." : *****\r\n"..Trans("user.update.fullname.old.info", "users").." : "..firstname.." "..lastname.."\r\n"..Trans("user.update.username.old.info", "users").." : "..username.."\r\n"..Trans("user.update.email.old.info", "users").." : "..email.."\r\n"..Trans("user.update.role.old.info", "users").." : "..role.."\r\n"..Trans("user.update.sign.old.info", "users").." : "..signCheckList.." / "..Trans("user.update.status.old.info", "users").." : "..status.."\r\n"..Trans("user.update.lastmodif.old.info", "users").." : "..lastModification.." "..lastModificationBy);
			row = mySQLCursor:fetch(row,"a");
		end
	end
end

--DXML.SetFilePath("UpdatingUsers", "AutoPlay\\Docs\\new_xml.xml");
--result = DXML.Save("UpdatingUsers");

local nNodes = DXML.Count("UpdatingUsers", "List/UpdatedUsers", "User");
for i=1,nNodes do
	local updatedUser = DXML.GetAttribute("UpdatingUsers", "List/UpdatedUsers/User:"..i, "Username");
	if username == updatedUser then
		local updatedUsername = DXML.GetAttribute("UpdatingUsers", "List/UpdatedUsers/User:"..i, "NewUsername");
		if updatedUsername ~= "" then
			Input.SetText("IN_USERNAME_UCF", updatedUsername);
		end

		local nChilds = DXML.Count("UpdatingUsers", "List/UpdatedUsers/User:"..i, "Field")
		for j=1, nChilds do
			local fieldToFill = DXML.GetAttribute("UpdatingUsers", "List/UpdatedUsers/User:"..i.."/Field:"..j, "Name");
			local textToFill = DXML.GetAttribute("UpdatingUsers", "List/UpdatedUsers/User:"..i.."/Field:"..j, "NewText");

			if fieldToFill == "Lastname" then
				Input.SetText("IN_LASTNAME_UCF", textToFill);
			end

			if fieldToFill == "Firstname" then
				Input.SetText("IN_FIRSTNAME_UCF", textToFill);
			end

			if fieldToFill == "Role" then
				ComboBox.SetSelected("CB_ROLE", ComboBox.FindItem("CB_ROLE", -1, LB_BYTEXT, textToFill));
			end

			if fieldToFill == "Sign check list" then
				if textToFill == "Yes" then
					Image.Load("IMG_TOGGLE_YES_NO", "AutoPlay\\htdocs\\images\\icons\\yes.png");
				else
					Image.Load("IMG_TOGGLE_YES_NO", "AutoPlay\\htdocs\\images\\icons\\no.png");
				end
			end

			if fieldToFill == "Status" then
				if textToFill == "Active" then
					Image.Load("IMG_CHECKBOX", "AutoPlay\\htdocs\\images\\icons\\checkbox_on.png");
				else
					Image.Load("IMG_CHECKBOX", "AutoPlay\\htdocs\\images\\icons\\checkbox_off.png");
				end
			end
		end			
	end
end

local checkUsername = Input.GetText("IN_USERNAME_UCF");
if checkUsername ~= "" then
	SQL = "SELECT * FROM IT_USERS WHERE Username = '"..checkUsername.."';";	

	mySQLCursor = mySQLConnection:execute(SQL);
	row = mySQLCursor:fetch({},"a")
	
	tblImageProps = {};
	tblImageProps.Visible = true;
	if row and row.Username ~= username then
		tblImageProps.ImageFile = "AutoPlay\\htdocs\\images\\icons\\error.png";
		tblImageProps.TooltipText = Trans("user.notification.exist", "users");
		Button.SetEnabled("BTN_UPDATE_USER", false);
	else
		tblImageProps.ImageFile = "AutoPlay\\htdocs\\images\\icons\\ok.png";
		tblImageProps.TooltipText = Trans("user.notification.available", "users");
		Button.SetEnabled("BTN_UPDATE_USER", true);
	end
	Image.SetProperties("IMG_NOTIFY", tblImageProps);
end
</Script>
