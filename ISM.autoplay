<Event>
<Name>On Key</Name>
<Args>number e_Key, table e_Modifiers</Args>
<Script>if e_Key == 9 then
	DialogEx.SetFocus("IN_FIRSTNAME_UCF");
end

if e_Key == 13 then
	DialogEx.ClickObject("BTN_UPDATE_USER");
end

local mySQLConnection = dbConnect();
if mySQLConnection ~= nil then
	local searchByFirstname = Input.GetText("IN_FIRSTNAME_UCF");
	local searchByLastname = Input.GetText("IN_LASTNAME_UCF");

	if (searchByFirstname ~= Trans("user.input.firstname", "users")) and (searchByLastname ~= Trans("user.input.lastname", "users")) then
		local username = String.Left(searchByFirstname, 1).."."..searchByLastname;
		Input.SetText("IN_USERNAME_UCF", "");
		Input.SetText("IN_USERNAME_UCF", username);

		SQL = "SELECT * FROM IT_USERS WHERE Username = '"..username.."';";	

		mySQLCursor = mySQLConnection:execute(SQL);
		row = mySQLCursor:fetch({},"a")

		tblImageProps = {};
		tblImageProps.Visible = true;
		if row then
			tblImageProps.ImageFile = "AutoPlay\\htdocs\\images\\icons\\error.png";
			tblImageProps.TooltipText = Trans("user.notification.exist", "users");
			Button.SetEnabled("BTN_UPDATE_USER", false);
		else
			tblImageProps.ImageFile = "AutoPlay\\htdocs\\images\\icons\\ok.png";
			tblImageProps.TooltipText = Trans("user.notification.available", "users");
			Button.SetEnabled("BTN_UPDATE_USER", true);
		end
		Image.SetProperties("IMG_NOTIFY", tblImageProps);
		mySQLCursor:close();
		mySQLConnection:close();
	end
end</Script>
