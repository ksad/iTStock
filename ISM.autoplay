<Event>
<Name>On Click</Name>
<Args/>
<Script>local imagePath = Dialog.FileBrowse(true, "", _SourceFolder.."\\AutoPlay\\htdocs\\images\\img_profil\\", "Image JPEG (*.jpg)|*.jpg|Image PNG (*.png)|*.*|All Files(*.*)|*.*|", "", "", false, false);

Debug.ShowWindow(true);

local image = String.SplitPath(imagePath[1]);
local nowDate = System.GetDate(DATE_FMT_YEAR)..System.GetDate(DATE_FMT_MONTH)..System.GetDate(DATE_FMT_DAY);
local nowTime = System.GetTime(TIME_FMT_HOUR)..System.GetTime(TIME_FMT_MIN)..System.GetTime(TIME_FMT_SEC);
local imageFileName = image.Filename.."_"..nowDate..nowTime..image.Extension;
Debug.Print(imageFileName.."\r\n");

File.Copy(imagePath[1], _SourceFolder.."\\AutoPlay\\htdocs\\images\\img_profil\\"..imageFileName, true, true, false, true, nil);

-- Test for error
error = Application.GetLastError();
if (error ~= 0) then
	Dialog.Message("Error", _tblErrorMessages[error], MB_OK, MB_ICONEXCLAMATION);
end

local mySQLConnection = dbConnect();
if mySQLConnection ~= nil then
	Debug.Print(CURRENT_USERNAME.."\r\n");
	SQL = "UPDATE `it_stock_manager`.`it_users` SET `Profil_Pict`='"..imageFileName.."' WHERE Username='"..CURRENT_USERNAME.."';";

	mySQLCursor, err = mySQLConnection:execute(SQL);
	if err then
		showMsgBox ("Error", "", Trans("common.update.pict.error", "common"), "OK");
		-- LOG_WRITE : err
		Application.ExitScript();
	else
		Image.Load("IMG_USER_PROFIL_PICT", "AutoPlay\\htdocs\\images\\img_profil\\"..imageFileName);
	end
end</Script>
