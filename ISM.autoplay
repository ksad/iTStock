<Event>
<Name>On Startup</Name>
<Args/>
<Script>require("Autoplay\\htdocs\\sources\\dlg_login");
require("Autoplay\\htdocs\\sources\\global_functions")

Application.SaveValue("ITStock", "FirstRun", "Y");
local LANG = TextFile.ReadToString("AutoPlay\\htdocs\\lang-properties\\default_lang.lang");
Application.SaveValue("ITStock", "LANG", LANG);

tblDgList = Application.GetDialogs();
for i,j in pairs(tblDgList) do
	Application.SaveValue("ITStock", "TRANS_FLAG_"..j, "NULL");
end

tblPgList = Application.GetPages();
for i,j in pairs(tblPgList) do
	Application.SaveValue("ITStock", "TRANS_FLAG_"..j, "NULL");
end</Script>
