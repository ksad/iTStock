<Event>
<Name>On Show</Name>
<Args/>
<Script>if Application.LoadValue("ITStock", "TRANS_FLAG_"..Application.GetCurrentDialog()) ~= "Y" then
	TransDialog("common");
end

local title = String.Lower(Application.LoadValue("MSGBOX", "title"));
local msgtitle = Application.LoadValue("MSGBOX", "msgtitle");
local msg = Application.LoadValue("MSGBOX", "msg");
local buttons = Application.LoadValue("MSGBOX", "buttons");
timer = Application.LoadValue("MSGBOX", "timer");

Image.Load("IMG_ICONE", "AutoPlay\\htdocs\\images\\msgbox\\"..title.."\\icone.png");
Image.Load("IMG_TITLE_BAR", "AutoPlay\\htdocs\\images\\msgbox\\"..title.."\\title_bar.png");
Image.Load("IMG_CLOSE", "AutoPlay\\htdocs\\images\\msgbox\\"..title.."\\close.png");
Label.SetText("LB_TITLE", Trans("common.msg.box.header."..String.Upper(title), "common"));

if msgtitle ~= "" then
	RichText.SetText("RT_MSG", "- "..msgtitle.." :\r\n"..msg, false);
else
	RichText.SetText("RT_MSG", msg, false);
end

if timer ~= "" then
	buttons = "";
	timer = String.ToNumber(timer);
	DialogEx.StartTimer(1000, 1);
end

local Line = 1;
while (true) do
	local textLine = RichText.GetLine("RT_MSG", Line);

	if textLine ~= "" then
		Line = Line + 1;
	else
		Line = Line - 1;
		break;
	end
end

RichText.SetSize("RT_MSG", RichText.GetSize("RT_MSG").Width, (RichText.GetSize("RT_MSG").Height * Line)+8);
Image.SetSize("IMG_BG_BODY", Image.GetSize("IMG_BG_BODY").Width, RichText.GetSize("RT_MSG").Height + 16);

-- RichText object properties table.
tblRichTextProps = {};
tblRichTextProps.ReadOnly = true;
--tblRichTextProps.Border = BORDER_SUNKEN;
RichText.SetProperties("RT_MSG", tblRichTextProps);

if buttons ~= "" then
	local Search = buttons;
	local Delim = 0;
	local Position = 1;

	tblButtons = {};
	while (true) do
		local Found = String.Find(Search, "|", Position, false);

		if (Found ~= -1) then
			Delim = Delim + 1;
			Position = Found + 1;
		else
			break;
		end
	end

	if Delim &gt; 0 then
		tblButtons = {};

		for i = 1, Delim + 1 do
			buttonName = String.Left(buttons, String.Find(buttons, "|", 1, false)-1);
			buttons = String.Replace(buttons, buttonName.."|", "", false)
			Table.Insert(tblButtons, i, buttonName);
			if String.Find(buttons, "|", 1, false) == -1 then
				Table.Insert(tblButtons, i+1, buttons);
				break;
			end
		end
	else
		tblButtons = {buttons};
	end

	-- Button object properties table.
	tblBtnProps = {};
	tblBtnProps.ButtonFile = "AutoPlay\\htdocs\\images\\msgbox\\"..title.."\\msgBox_button.btn";
	tblBtnProps.FontName = "Nunito";
	tblBtnProps.FontSize = 12;
	tblBtnProps.FontWeight = FW_NORMAL;
	tblBtnProps.FontItalic = false;
	tblBtnProps.FontStrikeout = false;
	tblBtnProps.FontScript = ANSI_CHARSET;
	tblBtnProps.FontAntiAlias = true;
	tblBtnProps.FontUnderline = false;
	tblBtnProps.XOffset = 0;
	tblBtnProps.YOffset = 0;
	tblBtnProps.LeftMargin = 0;
	tblBtnProps.RightMargin = 0;
	tblBtnProps.Style = BTNSTYLE_STANDARD;
	tblBtnProps.ToggleState = BTN_UP;
	tblBtnProps.Cursor = CURSOR_HAND;
	tblBtnProps.TooltipText = "";
	tblBtnProps.Height = 30;
	tblBtnProps.Enabled = true;
	tblBtnProps.Visible = true;
	tblBtnProps.ResizeLeft = false;
	tblBtnProps.ResizeRight = false;
	tblBtnProps.ResizeTop = false;
	tblBtnProps.ResizeBottom = false;
	tblBtnProps.HighlightSound = SND_STANDARD;
	tblBtnProps.HighlightSoundFile = "";
	tblBtnProps.ClickSound = SND_STANDARD;
	tblBtnProps.ClickSoundFile = "";
	tblBtnProps.ColorNormal = Math.HexColorToNumber("FFFFFF");
	tblBtnProps.ColorHighlight = Math.HexColorToNumber("FFFFFF");
	tblBtnProps.ColorDown = Math.HexColorToNumber("FFFFFF");
	tblBtnProps.ColorDisabled = Math.HexColorToNumber("FFFFFF");
	tblBtnProps.Alignment = ALIGN_CENTER;

	btnPosX = 0;
	for i,button in pairs (tblButtons) do
		tblBtnProps.Text = button;
		tblBtnProps.Width = 20 * String.Length(button);
		tblBtnProps.Y = Image.GetPos("IMG_BG_BODY").Y + Image.GetSize("IMG_BG_BODY").Height + 10;
		tblBtnProps.X = 0;
		DialogEx.CreateObject(OBJECT_BUTTON, "BTN_"..button, tblBtnProps);
		DialogEx.SetObjectScript("BTN_"..button, "On Click", 'Application.SaveValue("MSGBOX", "PRESSED_KEY", "ID"..Button.GetText(this));DialogEx.Close(this);');
		btnPosX = btnPosX + (20 * String.Length(button));
	end

	for i,button in pairs (tblButtons) do
		if i == 1 then
			Button.SetPos("BTN_"..button, (DialogEx.GetSize().Width - (btnPosX + (10 * Delim)))/ 2, Button.GetPos("BTN_"..button).Y);
		else
			Button.SetPos("BTN_"..button, Button.GetPos("BTN_"..tblButtons[i-1]).X + Button.GetSize("BTN_"..tblButtons[i-1]).Width + 10, Button.GetPos("BTN_"..button).Y);
		end
	end

	Window.SetSize(DialogEx.GetWndHandle(), DialogEx.GetSize().Width, Button.GetPos("BTN_"..tblButtons[1]).Y + Button.GetSize("BTN_"..tblButtons[1]).Height + 10);
else
	Window.SetSize(DialogEx.GetWndHandle(), DialogEx.GetSize().Width, Image.GetPos("IMG_BG_BODY").Y + Image.GetSize("IMG_BG_BODY").Height + 5);
end</Script>
